{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "autoplay=*, fullscreen=*, picture-in-picture=*, encrypted-media=*, clipboard-write=*"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-src https://www.youtube.com https://youtube.com https://*.vercel.app; frame-ancestors 'self'; object-src 'none'; base-uri 'self';"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}